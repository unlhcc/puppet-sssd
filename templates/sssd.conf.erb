[sssd]
config_file_version = 2
services = nss, pam
domains = LDAP


[nss]
<% if @client_idle_timeout -%>
client_idle_timeout = <%= @client_idle_timeout %>
<% end -%>
<% if @entry_negative_timeout -%>
entry_negative_timeout = <%= @entry_negative_timeout %>
<% end -%>


[pam]


[domain/LDAP]
id_provider = ldap
auth_provider = ldap
ldap_uri = <%= @ldap_uri %>
ldap_search_base = <%= @ldap_base %>
<% if @ldap_user_search_base -%>
ldap_user_search_base = <%= @ldap_user_search_base %>
<% end -%>
<% if @ldap_group_search_base -%>
ldap_group_search_base = <%= @ldap_group_search_base %>
<% end -%>
<% if @ldap_tls_reqcert -%>
ldap_tls_reqcert = <%= @ldap_tls_reqcert %>
<% end -%>
<% if @ldap_tls_cacert -%>
ldap_tls_cacert = <%= @ldap_tls_cacert %>
<% end -%>
<% if @ldap_tls_cacertdir -%>
ldap_tls_cacertdir = <%= @ldap_tls_cacertdir %>
<% end -%>
<% if @ldap_id_use_start_tls -%>
ldap_id_use_start_tls = <%= @ldap_id_use_start_tls %>
<% end -%>
cache_credentials = <%= @cache_credentials %>
enumerate = <%= @enumerate %>
<% if @entry_cache_timeout -%>
entry_cache_timeout = <%= @entry_cache_timeout %>
<% end -%>
<% if @refresh_expired_interval -%>
refresh_expired_interval = <%= @refresh_expired_interval %>
<% end -%>
